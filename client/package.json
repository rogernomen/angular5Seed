{
  "name": "b2b-bko",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod --aot",
    "build-local": "ng build --prod --aot --base-href .",
    "start-local-build": "ng serve --prod --aot -w -pc ./proxy/proxy-local.conf.json --open -lr -p 9000 -ssl true --host 0.0.0.0",
    "start-local": "ng serve --extract-css=true -w -pc ./proxy/proxy-local.conf.json --open -lr -p 9000 --host 0.0.0.0 --disableHostCheck true",
    "start-pre": "ng serve --extract-css=true -w -pc ./proxy/proxy-pre.conf.js --open -lr -p 9000 --host 0.0.0.0 --disableHostCheck true",
    "start-pro": "ng serve --extract-css=true -w -pc ./proxy/proxy-pro.conf.js --open -lr -p 9000 --host 0.0.0.0 --disableHostCheck true",
    "start-hmr-local": "ng serve --hmr --env=hmr -w -pc ./proxy/proxy-local.conf.json --open -lr -p 9000 --host 0.0.0.0 --disableHostCheck true",
    "start-hmr-local-gateway": "ng serve --hmr --env=hmr -w -pc ./proxy/proxy-local-gateway.conf.json --open -lr -p 9000 --host 0.0.0.0 --disableHostCheck true",
    "start-hmr-local-vagrant": "ng serve --hmr --env=hmr -w -pc ./proxy/proxy-local-vagrant.conf.json --open -lr -p 9000 --host 0.0.0.0 --disableHostCheck true",
    "start-hmr-pre": "ng serve --hmr --env=hmr -w -pc ./proxy/proxy-pre.conf.js --open -lr -p 9000 --host 0.0.0.0 --disableHostCheck true",
    "start-hmr-pro": "ng serve --hmr --env=hmr -w -pc ./proxy/proxy-pro.conf.js --open -lr -p 9000 --host 0.0.0.0 --disableHostCheck true",
    "test-deploy": "ng test",
    "test-local": "run-p test coverage-server",
    "test": "ng test --code-coverage",
    "test_ic": "ng test -sr",
    "coverage-server": "autoreload-server -r \"**/**\" -b http://localhost:9010/index.html -w \"**/**.{html,css}\" ./coverage 9010",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "@ngx-translate/core": "^9.1.1",
    "@ngx-translate/http-loader": "^2.0.1",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "json2typescript": "^1.0.5",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.14.105",
    "ngx-webstorage": "^2.0.1",
    "primeng": "^5.2.1",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "~1.7.0",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@angularclass/hmr": "^2.1.3",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/jwt-decode": "^2.2.1",
    "@types/lodash": "^4.14.105",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "https-proxy-agent": "2.1.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "~1.0.4",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "protractor": "~5.1.2",
    "simple-autoreload-server": "^0.2.7",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3"
  }
}
